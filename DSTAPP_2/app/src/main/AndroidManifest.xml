<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anurag.dstapp_2">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gos_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CardViewActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".QueueActivity"
            android:theme="@style/MyTabsTheme" />
        <activity
            android:name=".EventsActivity"
            android:theme="@style/MyTabsTheme" />
        <activity
            android:name=".PlacesActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".About_Us" />
        <activity
            android:name=".Imageviewer"
            android:theme="@style/AppTheme" />
        <activity android:name=".CoverpicViewer"></activity>
    </application>

</manifest>
